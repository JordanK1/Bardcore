@model IEnumerable<Bardcore.Models.UserProfile>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DisplayName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                <a asp-action="Show" asp-route-id="@item.Userid">@Html.DisplayFor(modelItem => item.DisplayName)</a>
            </td>
        </tr>
}
    </tbody>
</table>
